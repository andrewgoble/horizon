ont-size: 16px !important;\n  border-radius: 5px;\n}\n\n/* modal links */\n.rivo-modal-p a {\n  text-decoration: underline;\n}\n\n.rivo-modal-p a::after {\n  content: \".\";\n}\n\n/* === HERO === */\ndiv:has(> #rivo-lp-header) {\n  width: 100%;\n  padding: 40px;\n}\n.rivo-hero-banner-wrapper {\n  max-width: 1500px;\n}\n\nh1.rivo-hero-title {\n  display: flex;\n  flex-direction: column;\n  gap: 24px;\n}\n\n.rivo-span {\n  font-weight: 400;\n  font-size: 16px;\n  color: #e0a580;\n  letter-spacing: 0.25em;\n  text-transform: capitalize;\n}\n\n#rivo-ways-to-earn.rivo-content-wrapper {\n  border-top-right-radius: 12px;\n  border-top-left-radius: 12px;\n}\n\n#rivo-ways-to-redeem.rivo-content-wrapper {\n  border-bottom-right-radius: 12px;\n  border-bottom-left-radius: 12px;\n}\n\n.rivo-hero-banner-text {\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n  align-self: center;\n  min-width: auto;\n  padding: 0;\n}\n\n.rivo-hero-description {\n  padding: 30px 0;\n  max-width: 400px;\n  font-family: var(--rivo-subtitle-font-family), sans-serif;\n  letter-spacing: 0.43px;\n  line-height: 20px;\n}\n\n.rivo-desktop-justify-right {\n  justify-content: center;\n}\n\n#rivo-lp-header {\n  display: flex;\n  max-width: 1200px;\n  margin: 0 auto;\n}\n\n.rivo-hero-banner-media {\n  position: relative;\n  max-height: 500px;\n  margin: auto;\n  border-radius: 12px;\n  order: 2;\n}\n\n.rivo-hero-banner-media img {\n  object-position: center;\n}\n\n.rivo-lp-section-subtitle {\n  width: 100%;\n  padding: 20px 0;\n}\n\n.rivo-hero-banner-wrapper {\n  padding: 0;\n}\n\n/* === TITLES === */\n#rivo-how-it-works {\n  width: 100%;\n}\n\n.rivo-section-label {\n  display: block;\n  font-size: 24px;\n  color: var(--rivo-secondary-color);\n  font-weight: 600;\n}\n\n/* === How it works === */\n\n.rivo-step-item {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n/* === CARDS === */\n.rivo-card {\n  flex-basis: 23%;\n}\n\n.rivo-card-title {\n  text-transform: capitali